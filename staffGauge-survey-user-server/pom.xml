            <artifactId>mail</artifactId>
            <version>1.4.7</version>
        </dependency>
        <dependency>
            <groupId>com.staffGauge.survey</groupId>
            <artifactId>staffGauge-survey-user</artifactId>
            <version>1.0-SNAPSHOT</version>
