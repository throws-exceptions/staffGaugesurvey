,YAAA,CAAA;AAAA,QAAA,QAAA,EAAA,KAAA,YAAA,CAAA;AAAA,OAAA,EAAA,KAAA,KAAA,CAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OAPA;AAUA,WAAA,aAAA,GAAA,KAAA;AACA,KAnCA;AAoCA,IAAA,YApCA,wBAoCA,KApCA,EAoCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA,MAAA,CAAA,YAAA,CAAA;AAAA,kBAAA,QAAA,EAAA,MAAA,CAAA,YAAA,CAAA;AAAA,iBAAA,EAAA,KAAA,CADA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,qBAAA,EAAA,QAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;;AAGA,oBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,SAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA5CA;AA6CA,IAAA,YA7CA,wBA6CA,KA7CA,EA6CA,KA7CA,EA6CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,EAAA,OAAA,CAAA,IAAA,EAAA,GAAA,CAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,aAAA,OAAA;AACA,KAnDA;AAoDA,IAAA,kBApDA,8BAoDA,IApDA,EAoDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,KAtDA;AAuDA,IAAA,gBAvDA,4BAuDA,IAvDA,EAuDA,CACA,CAxDA;AAyDA,IAAA,QAzDA,sBAyDA;AAAA;;AACA,WAAA,KAAA,CAAA,YAAA,CAAA,QAAA;AAAA,2EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAEA,kBAAA,MAAA,CAAA,UAAA,GAAA,IAAA;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AAHA;AAAA,yBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,MAAA,CAAA,YAAA,CAJA;;AAAA;AAIA,kBAAA,QAJA;AAKA,kBAAA,GALA,GAKA,QALA,CAKA,IALA;AAMA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,sBAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,KACA,IAAA,GAAA,CAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,KACA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAWA,KArEA;AAsEA,IAAA,cAtEA,4BAsEA;AAAA;;AACA,WAAA,KAAA,CAAA,YAAA,CAAA,QAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAEA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;;AAFA;;AAAA;AAAA,wBAKA,MAAA,CAAA,YAAA,CAAA,IAAA,KAAA,MAAA,CAAA,YAAA,CAAA,OAAA,IAAA,MAAA,CAAA,IALA;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAMA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,qBAAA,EAAA,MAAA,CAAA,YAAA,CANA;;AAAA;AAMA,kBAAA,QANA;;AAOA,sBAAA,QAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,WAAA;AACA;;AATA;AAAA;;AAAA;AAUA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAaA,WAAA,iBAAA,GAAA,KAAA;AACA,KArFA;AAsFA,IAAA,UAtFA,wBAsFA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,0BAAA,MAAA,CAAA,YAAA,CAAA,IADA;AAEA,0BAAA,MAAA,CAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;;AAKA,oBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA;;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,KAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAjGA;AA5CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-card shadow=\"hover\" class=\"mgb20\" style=\"height:252px;\">\r\n      <div class=\"user-info-list\">\r\n        <img :src=\"this.userInfoForm.head\" class=\"user-avator\" alt/>\r\n        <div class=\"user-info-list\">\r\n          <div class=\"user-info-name\">{{this.userInfoForm.name}}</div>\r\n          <div>{{this.userInfoForm.permission}}</div>\r\n          <div>{{this.userInfoForm.mail}}</div>\r\n          <el-button type=\"text\" @click=\"dialogUserVisible=true\">编辑资料</el-button>\r\n          <el-dialog title=\"编辑个人资料\" :visible.sync=\"dialogUserVisible\">\r\n            <el-form ref=\"userInfoForm\" :model=\"userInfoForm\" :rules=\"userInfoFormRules\">\r\n              <img :src=\"this.userInfoForm.head\" class=\"user-avator\" alt/>\r\n              <el-upload\r\n                class=\"upload-demo\"\r\n                action=\"http://localhost:8081/user/up\"\r\n                :data=\"{username: this.userInfoForm.name}\"\r\n                accept=\"image/png,image/gif,image/jpg,image/jpeg\"\r\n                :auto-upload=\"true\"\r\n                :limit=\"1\"\r\n                :on-success=\"handleSuccess\">\r\n                <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n                <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过2MB</div>\r\n              </el-upload>\r\n              <el-form-item label=\"用户名\" label-width=\"120px\">\r\n                <el-input v-model=\"userInfoForm.newname\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"邮箱\" label-width=\"120px\">\r\n                <el-row>\r\n                  <el-col :span=\"15\">\r\n                    <el-input v-model=\"userInfoForm.newmail\"></el-input>\r\n                  </el-col>\r\n                  <el-col :span=\"5\">\r\n                    <el-button type=\"text\" @click=\"sendCode\">验证</el-button>\r\n                  </el-col>\r\n                  <el-dialog title=\"输入验证码\" :visible.sync=\"dialogCode\" append-to-body>\r\n                    <el-input v-model=\"Code\"></el-input>\r\n                    <div slot=\"footer\" class=\"dialog-footer\">\r\n                      <el-button @click=\"dialogCode = false\">取 消</el-button>\r\n                      <el-button type=\"primary\" @click=\"updateCode\">确 定</el-button>\r\n                    </div>\r\n                  </el-dialog>\r\n                </el-row>\r\n              </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n              <el-button @click=\"dialogUserVisible = false\">取 消</el-button>\r\n              <el-button type=\"primary\" @click=\"updateUserInfo\">确 定</el-button>\r\n            </div>\r\n          </el-dialog>\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n    <el-card shadow=\"hover\">\r\n      <vue-event-calendar :events=\"todoList\" @day-changed=\"handleDayChanged\" @month-changed=\"handleMonthChanged\"\r\n                          class=\"events-calendar\">\r\n        <template scope=\"props\">\r\n          <div v-for=\"(event, index) in props.showEvents\" :key=\"index\" class=\"event-item\">\r\n            <!-- In here do whatever you want, make you owner event template -->\r\n            <div class=\"wrapper\">\r\n              <h3 class=\"title\">{{index+1}}.{{event.title}}</h3>\r\n              <p class=\"time\">{{event.date}}</p>\r\n              <p class=\"desc\">{{event.desc}}</p>\r\n              <el-button type=\"text\" icon=\"el-icon-edit\" circle\r\n                         @click=\"dialogVisible=true,Event=event\"></el-button>\r\n              <el-dialog title=\"编辑待办事项\" :visible.sync=\"dialogVisible\">\r\n                <el-form :model=\"Event\">\r\n                  <el-form-item label=\"日期\" label-width=\"120px\">\r\n                    <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"Event.date\"\r\n                                    style=\"width: 100%;\" value-format=\"yyyy/MM/dd\"\r\n                                    change=\"yyyy/MM/dd\"></el-date-picker>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"标题\" label-width=\"120px\">\r\n                    <el-input v-model=\"Event.title\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"内容\" label-width=\"120px\">\r\n                    <el-input type=\"textarea\" v-model=\"Event.desc\"></el-input>\r\n                  </el-form-item>\r\n                </el-form>\r\n                <div slot=\"footer\" class=\"dialog-footer\">\r\n                  <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n                  <el-button type=\"primary\" @click=\"updateToDo\">确 定</el-button>\r\n                </div>\r\n              </el-dialog>\r\n              <el-button type=\"text\" icon=\"el-icon-success\" circle @click=\"deleteToList(event)\"></el-button>\r\n            </div>\r\n          </div>\r\n          <el-button type=\"text\" @click=\"dialogFormVisible = true\" icon=\"el-icon-circle-plus\"></el-button>\r\n          <el-dialog title=\"添加待办事项\" :visible.sync=\"dialogFormVisible\" custom-class=\"insert\">\r\n            <el-form :model=\"Event\">\r\n              <el-form-item label=\"日期\" label-width=\"120px\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"Event.date\"\r\n                                style=\"width: 100%;\" value-format=\"yyyy/MM/dd\" change=\"yyyy/MM/dd\"></el-date-picker>\r\n              </el-form-item>\r\n              <el-form-item label=\"标题\" label-width=\"120px\">\r\n                <el-input v-model=\"Event.title\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"内容\" label-width=\"120px\">\r\n                <el-input type=\"textarea\" v-model=\"Event.desc\"></el-input>\r\n              </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n              <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n              <el-button type=\"primary\" @click=\"addToList\">确 定</el-button>\r\n            </div>\r\n          </el-dialog>\r\n        </template>\r\n      </vue-event-calendar>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n  export default {\r\n    name: 'dashboard',\r\n    data() {\r\n      return {\r\n\r\n        todoList: [{date: '2020/04/14', title: '测试', desc: '测试日历功能'},\r\n          {date: '2020/04/15', title: '测试', desc: '测试日历功能'}],\r\n        Event: {date: '', title: '', desc: ''},\r\n        dialogFormVisible: false,\r\n        dialogVisible: false,\r\n        dialogUserVisible: false,\r\n        dialogCode: false,\r\n        Code: '',\r\n        userInfoForm: {\r\n          name: sessionStorage.getItem('username'),\r\n          newname: '傅杰青',\r\n          head: require('@/assets/avator/' + sessionStorage.getItem('head')),\r\n          permission: sessionStorage.getItem('permission') == 'A' ? '管理员' : '普通用户',\r\n          mail: '1163107972@qq.com',\r\n          newmail: '1163107972@qq.com'\r\n        },\r\n        userInfoFormRules: {\r\n          mail: [\r\n            {required: true, message: '请输入邮箱地址', trigger: 'blur'},\r\n            {type: 'email', message: '请输入正确的邮箱地址', trigger: ['blur', 'change']}\r\n          ],\r\n          name: [\r\n            {required: true, message: '请输入用户名!', trigger: 'blur'},\r\n            {min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur'}\r\n          ]\r\n        }\r\n      };\r\n    },\r\n    created() {\r\n      this.getUserInfo();\r\n      this.getToList();\r\n    },\r\n    // activated() {\r\n    //     this.handleListener();\r\n    // },\r\n    // deactivated() {\r\n    //     window.removeEventListener('resize', this.renderChart);\r\n    //     bus.$off('collapse', this.handleBus);\r\n    // },\r\n    methods: {\r\n      async getUserInfo() {\r\n        const {data: res} = await this.$http.get('user/info', {params: {\"username\": sessionStorage.getItem('username')}})\r\n        window.sessionStorage.setItem('head', res.head)\r\n        window.sessionStorage.setItem('permission', res.permission)\r\n        window.sessionStorage.setItem('username', res.username)\r\n      },\r\n      getToList() {\r\n        this.$http.get('user/getToDoList', {params: {username: this.userInfoForm.name}}).then(response => {\r\n          console.log(response.data.list)\r\n          this.todoList = response.data.list\r\n          console.log(this.todoList)\r\n        })\r\n      },\r\n      async addToList() {\r\n        const {data: res} = await this.$http.post('user/addToDoList', this.twoJsonMerge({username: this.userInfoForm.name}, this.Event))\r\n        if (res.code === 200) {\r\n          this.getToList()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n        this.dialogFormVisible = false;\r\n      },\r\n      updateToDo() {\r\n        this.$http.post('user/updateToDoList', this.twoJsonMerge({username: this.userInfoForm.name}, this.Event)).then(\r\n          response => {\r\n            if (response.data.code === 200) {\r\n              this.getToList()\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          }\r\n        )\r\n\r\n        this.dialogVisible = false;\r\n      },\r\n      async deleteToList(event) {\r\n        var resEvent = this.twoJsonMerge({username: this.userInfoForm.name}, event)\r\n        const {data: res} = await this.$http.post('user/deleteToDoList', resEvent)\r\n        if (res.code === 200) {\r\n          this.getToList()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      },\r\n      twoJsonMerge(json1, json2) {\r\n        console.log(json1)\r\n        console.log(json2)\r\n        var jsonStr = JSON.parse((JSON.stringify(json1) + JSON.stringify(json2)).replace(/}{/, ','));\r\n        console.log(jsonStr)\r\n        return jsonStr\r\n      },\r\n      handleMonthChanged(data) {\r\n        console.log(data);\r\n      },\r\n      handleDayChanged(data) {\r\n      },\r\n      sendCode() {\r\n        this.$refs.userInfoForm.validate(async valid => {\r\n          if (!valid) return\r\n          this.dialogCode = true\r\n          this.$message.success('正在发送验证码')\r\n          const response = await this.$http.post('user/verifyCode', this.userInfoForm)\r\n          const {data: res} = response\r\n          console.log(res)\r\n          if (res.code == 200) this.$message.success(res.msg)\r\n          else if (res.code) this.$message.error(res.msg)\r\n          else this.$message.info(res.msg)\r\n        })\r\n      },\r\n      updateUserInfo() {\r\n        this.$refs.userInfoForm.validate(async valid => {\r\n          if (!valid) {\r\n            this.$message.error(\"用户信息更新失败\")\r\n            return\r\n          }\r\n          if (this.userInfoForm.mail === this.userInfoForm.newmail || this.flag) {\r\n            const response = await this.$http.post('user/updateUserInfo', this.userInfoForm)\r\n            if (response.data.code === 200) {\r\n              this.getUserInfo()\r\n            }\r\n          } else this.$message.error(\"用户信息更新失败\")\r\n\r\n        })\r\n        this.dialogUserVisible = false\r\n      },\r\n      async updateCode() {\r\n        var {data: res} = await this.$http.post('user/updateCode', {\r\n          'name': this.userInfoForm.name,\r\n          'Code': this.Code\r\n        })\r\n        if (res.code === 200) {\r\n          this.flag = true\r\n        } else {\r\n          this.flag = false\r\n        }\r\n        this.dialogCode = false\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n\r\n<style scoped>\r\n  .el-row {\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .user-info {\r\n    display: flex;\r\n    align-items: center;\r\n    padding-bottom: 0px;\r\n    border-bottom: 2px solid #ccc;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .user-avator {\r\n    height: 60px;\r\n    width: 60px;\r\n    border-radius: 50%;\r\n  }\r\n\r\n\r\n  .user-info-cont {\r\n    padding-left: 10px;\r\n    flex: 1;\r\n    font-size: 14px;\r\n    color: #999;\r\n  }\r\n\r\n  .user-info-cont div:first-child {\r\n    font-size: 30px;\r\n    color: #222;\r\n  }\r\n\r\n  .user-info-list {\r\n    font-size: 14px;\r\n    color: #999;\r\n    line-height: 25px;\r\n  }\r\n\r\n  .user-info-list span {\r\n    margin-left: 70px;\r\n  }\r\n\r\n  .mgb20 {\r\n    margin-bottom: 10px;\r\n    height: 130px;\r\n  }\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}