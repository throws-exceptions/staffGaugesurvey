SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KAVA;AAWA;AACA,IAAA,mBAZA,+BAYA,OAZA,EAYA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KAfA;AAgBA,IAAA,SAhBA,qBAgBA,GAhBA,EAgBA,MAhBA,EAgBA;AACA,aAAA,GAAA,CAAA,UAAA,KAAA,GAAA,GAAA,KAAA,GAAA,MAAA;AACA,KAlBA;AAmBA,IAAA,WAnBA,uBAmBA,CAnBA,EAmBA,CAnBA,EAmBA;AACA,aAAA,CAAA,CAAA,UAAA,KAAA,GAAA,GAAA,CAAA,GAAA,CAAA,CAAA;AACA,KArBA;AAsBA,IAAA,SAtBA,qBAsBA,QAtBA,EAsBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AAAA,2BAAA,MAAA,CAAA,QAAA;AAAA,8BAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,oBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA,MAAA,CAAA,WAAA,GAAA,KACA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA1BA;AA2BA,IAAA,YA3BA,wBA2BA,QA3BA,EA2BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AAAA,2BAAA,MAAA,CAAA,QAAA;AAAA,8BAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,oBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA,MAAA,CAAA,WAAA,GAAA,KACA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA/BA;AArBA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-card class=\"user-card\">\n      <el-row :gutter=\"5\" class=\"user-row\">\n        <el-col :span=\"7\">\n          <el-input\n            size=\"mini\"\n            v-model=\"search\"\n            placeholder=\"输入用户名关键字搜索\"/>\n        </el-col>\n      </el-row>\n\n      <el-table\n        :data=\"userList.filter(data => !search || data.userName.toLowerCase().includes(search.toLowerCase()))\"\n        border\n        :default-sort=\"{prop: 'userName', order: 'Ascending'}\">\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\n        <el-table-column\n          prop=\"userName\"\n          label=\"姓名\">\n        </el-table-column>\n        <el-table-column\n          prop=\"mail\"\n          label=\"邮箱\">\n        </el-table-column>\n        <el-table-column\n          prop=\"phoneNumber\"\n          label=\"电话\">\n        </el-table-column>\n        <el-table-column\n          prop=\"permissionName\"\n          label=\"角色\"\n          :formatter=\"isManager\"\n          sortable\n          :sort-method=\"sortManager\"\n        >\n        </el-table-column>\n        <el-table-column\n          label=\"操作\">\n          <template slot-scope=\"scope\">\n            <el-row>\n              <el-button type=\"success\" icon=\"el-icon-top\" size=\"small\" @click=\"addManger(scope.row.userName)\">升级\n              </el-button>\n            </el-row>\n            <el-button type=\"danger\" size=\"small\" icon=\"el-icon-bottom\" @click=\"deleteManger(scope.row.userName)\">降级\n            </el-button>\n\n          </template>\n        </el-table-column>\n      </el-table>\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"queryInfo.pageNum\"\n        :page-sizes=\"[1, 2, 5, 10]\"\n        :page-size=\"queryInfo.pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\">\n      </el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: \"UserManager\",\n    data() {\n      return {\n        username: sessionStorage.getItem(\"username\"),\n        queryInfo: {\n          //查询数据\n          query: '',\n          //当前页数\n          pageNum: 1,\n          pageSize: 5\n        },\n        userList: [],\n        total: 0,\n        permissionName: '',\n        search: ''\n      }\n    },\n    created() {\n      this.getUserList()\n    },\n    methods: {\n      async getUserList() {\n        const {data: res} = await this.$http.get('user/getUserList', {params: this.queryInfo})\n        this.userList = res.list\n        this.total = res.total\n      },\n      //监听pagesize改变的事件\n      handleSizeChange(newSize) {\n        this.queryInfo.pageSize = newSize\n        this.getUserList()\n      },\n      //监听page改变的事件\n      handleCurrentChange(newPage) {\n        this.queryInfo.pageNum = newPage\n        this.getUserList()\n      },\n      isManager(row, column) {\n        return row.permission === 'A' ? '管理员' : '普通用户'\n      },\n      sortManager(a, b) {\n        return a.permission === 'A' ? 1 : -1\n      },\n      async addManger(row_name) {\n        var {data: res} = await this.$http.post('user/addManager', {\"admin\": this.username, \"username\": row_name})\n        if (res.code === 200) this.getUserList()\n        else this.$message.error(res.msg)\n      },\n      async deleteManger(row_name) {\n        var {data: res} = await this.$http.post('user/deleteManager', {\"admin\": this.username, \"username\": row_name})\n        if (res.code === 200) this.getUserList()\n        else this.$message.error(res.msg)\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .user-card /deep/ .user-row {\n    margin-bottom: 10px;\n  }\n</style>\n"],"sourceRoot":"src/components/Users"}]}