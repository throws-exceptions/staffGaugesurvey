 'Ascending'}\">\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\n        <el-table-column\n          prop=\"person\"\n          label=\"处理人\">\n        </el-table-column>\n        <el-table-column\n          prop=\"freightersNum\"\n          label=\"轮船型号\">\n        </el-table-column>\n        <el-table-column\n          prop=\"startWeight\"\n          label=\"初始载重量\">\n        </el-table-column>\n        <el-table-column\n          prop=\"endWeight\"\n          label=\"最终载重量\"\n        >\n        </el-table-column>\n        <el-table-column\n          prop=\"goodsWeight\"\n          label=\"货物载重量\"\n        >\n        </el-table-column>\n        <el-table-column\n          prop=\"time\"\n          label=\"时间\"\n        >\n        </el-table-column>\n      </el-table>\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"queryInfo.pageNum\"\n        :page-sizes=\"[1, 2, 5, 10]\"\n        :page-size=\"queryInfo.pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\">\n      </el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: \"UserManager\",\n    data() {\n      return {\n        person: sessionStorage.getItem(\"username\"),\n        queryInfo: {\n          //查询数据\n          query: '',\n          //当前页数\n          pageNum: 1,\n          pageSize: 5\n        },\n        cargoList: [],\n        total: 0,\n        permissionName: '',\n        search: ''\n      }\n    },\n    created() {\n      this.getCargoList()\n    },\n    methods: {\n      async getCargoList() {\n        const {data: res} = await this.$http.get('http://localhost:8085/cargo/getCargoList', {params: this.queryInfo})\n        this.cargoList = res.list\n        this.total = res.total\n      },\n      //监听pagesize改变的事件\n      handleSizeChange(newSize) {\n        this.queryInfo.pageSize = newSize\n        this.getCargoList()\n      },\n      //监听page改变的事件\n      handleCurrentChange(newPage) {\n        this.queryInfo.pageNum = newPage\n        this.getCargoList()\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .cargo-card /deep/ .user-row {\n    margin-bottom: 10px;\n  }\n</style>\n"]}]}