请输入内容\"></el-input>-->\n      <!--        </el-col>-->\n      <!--        <el-col :span=\"4\">-->\n      <!--          <el-button type=\"primary\" round>搜索用户</el-button>-->\n      <!--        </el-col>-->\n      <!--        <el-col :span=\"4\">-->\n      <!--          <el-button type=\"primary\" round>添加用户</el-button>-->\n      <!--        </el-col>-->\n      <!--      </el-row>-->\n      <div>\n        <el-table\n          :data=\"userList\"\n          style=\"width: auto;height: auto\"\n          class=\"user-table\">\n          <el-table-column\n            prop=\"userName\"\n            label=\"姓名\"\n            width=\"120\">\n          </el-table-column>\n          <el-table-column\n            prop=\"mail\"\n            label=\"邮箱\"\n            width=\"120\">\n          </el-table-column>\n          <el-table-column\n            prop=\"phoneNumber\"\n            label=\"电话\"\n            width=\"120\">\n          </el-table-column>\n          <el-table-column\n            prop=\"permission\"\n            label=\"角色\"\n            width=\"300\">\n          </el-table-column>\n          <el-table-column\n            label=\"操作\"\n            width=\"100\">\n            <template>\n              <el-button type=\"text\" size=\"small\">升级权限</el-button>\n              <el-button type=\"text\" size=\"small\">降级权限</el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n        <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"queryInfo.pageNum\"\n          :page-sizes=\"[1, 2, 5, 10]\"\n          :page-size=\"queryInfo.pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n        </el-pagination>\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: \"UserManager\",\n    data() {\n      return {\n        queryInfo: {\n          //当前页数\n          pageNum: 1,\n          pageSize: 5\n        },\n        userList: [],\n        total: 0\n      }\n    },\n    created() {\n      this.getUserList()\n    },\n    methods: {\n      async getUserList() {\n        const {data: res} = await this.$http.get('user/getUserList', {params: this.queryInfo})\n        this.userList = res.list\n        this.total = res.total\n      },\n      //监听pagesize改变的事件\n      handleSizeChange(newSize) {\n        this.queryInfo.pageSize = newSize\n        this.getUserList()\n      },\n      //监听page改变的事件\n      handleCurrentChange(newPage) {\n        this.queryInfo.pageNum = newPage\n        this.getUserList()\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n  .user-table {\n    padding: 0;\n  }\n\n</style>\n"]}]}